extends base.pug 

block script
  script(src="/static/index.js")

block style
    style
      include ../static/style.css
      include ../static/contactstyle.css 
block content
  h1 Contact Us.  
  form(action="/contact",method="post",class="myform")
    input(type="text",class="myinput",name="Name",placeholder="enter your name") 
    input(type="text",class="myinput",name="Age",placeholder="enter your age")    
    input(type="text",class="myinput",name="phone No.",placeholder="enter your phone no.")    
    input(type="text",class="myinput",name="Email",placeholder="enter your Email")   
    input(type="text",class="myinput",name="Intrested Course",placeholder="enter your Intrested Course") 
    button(class="btn")
      | Submit                      